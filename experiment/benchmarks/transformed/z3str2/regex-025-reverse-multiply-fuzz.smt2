(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.+ (re.++ (str.to.re "bb") (re.* (str.to.re "?"))))))
(assert (str.in.re y (re.* (re.++ (str.to.re ";$b") (re.* (str.to.re "aa"))))))
(assert (= (str.to.int x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 2))
(check-sat)
(get-model)
