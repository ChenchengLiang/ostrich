(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.* (re.++ (re.* (str.to.re "aa")) (str.to.re "b")))))
(assert (str.in.re y (re.+ (re.union (re.* (str.to.re "a' 'a")) (str.to.re "b")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 0))
(check-sat)
(get-model)
