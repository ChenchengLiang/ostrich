(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.len Username) 64))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 256))
(assert (> (str.len Domainname) 0))
(assert (= Email (str.++ (str.++ Username Domainname) "a")))
(assert (str.in.re Email (re.++ (re.union (re.union (re.range "0" "9") (re.range "a" "z")) (re.+ (re.++ (re.++ (str.to.re "a") (re.++ (re.+ (re.+ (re.union (re.range "A" "Z") (re.union (re.range "0" "9") (re.range "a" "z"))))) (re.union (re.union (re.range "0" "9") (re.range "a" "z")) (re.+ (str.to.re "'\r'"))))) (re.range "A" "Z")))) (re.range "A" "Z"))))
(assert (not (str.in.re Domainname (re.++ re.allchar (re.* (re.++ (re.* (str.to.re "'\r'")) re.allchar))))))
(check-sat)
