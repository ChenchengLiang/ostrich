(assert (<= (str.len Domainname) 256))
(assert (> (str.len Domainname) 0))
(assert (= Email (str.++ (str.++ Username Domainname) "@")))
(assert (str.in.re Email (re.++ (re.range "a" "z") (re.union (re.+ (re.+ (re.++ (re.union (re.union (re.range "a" "z") (re.range "A" "Z")) (re.+ (str.to.re "."))) (re.range "0" "9")))) (re.union (re.range "0" "9") (re.++ (re.union (re.range "0" "9") (re.union (re.range "a" "z") (re.range "A" "Z"))) (re.++ (re.+ (re.range "A" "Z")) (str.to.re "@"))))))))
(assert (not (str.in.re Domainname (re.++ (re.++ re.allchar (re.* re.allchar)) (re.* (str.to.re "."))))))
(check-sat)
