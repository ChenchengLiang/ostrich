(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.len Username) 1))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 509))
(assert (> (str.to.int Domainname) 0))
(assert (= Email (str.++ Domainname (str.++ Username ""))))
(assert (str.in.re Email (re.union (re.union (re.range "a" "z") (re.+ (re.union (re.union (re.union (re.+ (str.to.re ".")) (re.++ (re.range "0" "9") (re.+ (re.+ (re.union (re.range "0" "9") (re.++ (re.range "a" "z") (re.range "A" "Z"))))))) (str.to.re "")) (re.union (re.range "a" "z") (re.range "A" "Z"))))) (re.union (re.range "0" "9") (re.range "A" "Z")))))
(assert (not (str.in.re Domainname (re.union (str.to.re "{") (re.union (re.* re.allchar) (re.+ re.allchar))))))
(check-sat)
