(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.to.int Username) 11))
(assert (> (str.len Username) 0))
(assert (<= (str.to.int Domainname) 46))
(assert (> (str.to.int Domainname) 0))
(assert (= Email (str.++ Username (str.++ "" Domainname))))
(assert (str.in.re Email (re.union (re.union (re.union (re.* (re.++ (re.union (re.range "A" "Z") (re.range "0" "9")) (re.range "a" "z"))) (re.+ (re.++ (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))) (str.to.re ".")))) (str.to.re "h")) (re.+ (re.union (re.range "a" "z") (re.++ (re.range "0" "9") (re.range "A" "Z")))))))
(assert (not (str.in.re Domainname (re.++ (re.+ re.allchar) (re.++ (str.to.re "w") (re.+ re.allchar))))))
(check-sat)
