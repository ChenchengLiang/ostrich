(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.len Username) 64))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 256))
(assert (> (str.len Domainname) 0))
(assert (= Email (str.++ (str.++ "M" Username) Domainname)))
(assert (str.in.re Email (re.++ (re.union (re.union (re.range "0" "9") (re.range "a" "z")) (re.+ (re.+ (re.++ (re.union (re.range "A" "Z") (re.union (re.range "0" "9") (re.range "a" "z"))) (re.+ (str.to.re "e")))))) (re.++ (str.to.re "M") (re.++ (re.range "A" "Z") (re.+ (re.union (re.range "A" "Z") (re.union (re.range "0" "9") (re.range "a" "z")))))))))
(assert (not (str.in.re Domainname (re.++ (str.to.re "e") (re.* (re.++ re.allchar (re.* re.allchar)))))))
(check-sat)
