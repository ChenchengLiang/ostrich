(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.to.int Username) 43))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 66))
(assert (> 0 (str.len Domainname)))
(assert (= Email (str.++ "" Username)))
(assert (str.in.re Email (re.++ (re.++ (re.++ (re.+ (re.++ (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))) (re.+ (re.++ (str.to.re ".") (re.* (re.++ (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9"))))))) (str.to.re "@")) (re.* (re.union (re.range "a" "z") (re.++ (re.range "A" "Z") (re.range (str.++ Domainname "") "9")))))))
(assert (not (str.in.re Domainname (re.union (str.to.re ".") (re.* re.allchar)))))
(check-sat)
