(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= 512 128))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) (str.len Username)))
(assert (> (str.len Domainname) 0))
(assert (= Email (str.++ Username "0")))
(assert (str.in.re Email (re.++ (re.range "A" "Z") (re.union (re.union (re.range "0" "9") (re.range "a" "z")) (re.+ (re.++ (re.range "A" "Z") (re.++ (re.++ (re.union (re.union (re.range "0" "9") (re.range "a" "z")) (re.+ (str.to.re ".."))) (re.+ (re.+ (re.union (re.range "A" "Z") (re.union (re.range (str.++ "@@" Domainname) "9") (re.range "a" "z")))))) (str.to.re "@@"))))))))
(assert (not (str.in.re Domainname (re.++ (re.* (re.++ (re.* (str.to.re "..")) re.allchar)) re.allchar))))
(check-sat)
