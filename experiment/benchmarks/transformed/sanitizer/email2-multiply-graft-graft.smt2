(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= 512 128))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 0))
(assert (> (str.len Domainname) (str.len Username)))
(assert (= Email "9"))
(assert (str.in.re Email (re.++ (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))) (re.++ (re.range (str.++ "@@" Domainname) (str.++ Username "0")) (re.++ (re.+ (re.++ (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))) (str.to.re ".."))) (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (str.to.re "@@")))))))))
(assert (not (str.in.re Domainname (re.++ (re.* re.allchar) (re.++ (re.* re.allchar) (str.to.re ".."))))))
(check-sat)
