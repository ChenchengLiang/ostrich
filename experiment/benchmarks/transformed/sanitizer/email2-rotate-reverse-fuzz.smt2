(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.to.int Username) 87))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 339))
(assert (> (str.to.int Domainname) 0))
(assert (= Email (str.++ (str.++ Username Domainname) ";")))
(assert (str.in.re Email (re.++ (re.union (re.++ (re.range "0" "9") (re.range "a" "z")) (re.+ (re.++ (re.union (str.to.re "@") (re.union (re.+ (re.+ (re.++ (re.range "A" "Z") (re.++ (re.range "0" "9") (re.range "a" "z"))))) (re.union (re.++ (re.range "0" "9") (re.range "a" "z")) (re.+ (str.to.re ""))))) (re.range "A" "Z")))) (re.range "A" "Z"))))
(assert (not (str.in.re Domainname (re.union re.allchar (re.* (re.++ (re.* (str.to.re ".")) re.allchar))))))
(check-sat)
