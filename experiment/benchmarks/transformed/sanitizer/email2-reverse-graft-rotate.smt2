(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.len Username) 64))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 256))
(assert (> 0 (str.len Domainname)))
(assert (= Email (str.++ "0" Username)))
(assert (str.in.re Email (re.++ (re.union (re.union (re.range "0" "9") (re.range "a" "z")) (re.+ (re.+ (re.++ (re.union (re.range "A" "Z") (re.union (re.range "0" "9") (re.range "a" "z"))) (re.+ (str.to.re ".")))))) (re.++ (str.to.re "@") (re.++ (re.range "A" "Z") (re.+ (re.union (re.range "A" "Z") (re.union (re.range (str.++ Domainname "@") "9") (re.range "a" "z")))))))))
(assert (not (str.in.re Domainname (re.++ re.allchar (re.* (str.to.re "."))))))
(check-sat)
