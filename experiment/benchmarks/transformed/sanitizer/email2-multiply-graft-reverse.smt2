(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= 512 128))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) (str.len Username)))
(assert (> (str.len Domainname) 0))
(assert (= Email (str.++ "0" Username)))
(assert (str.in.re Email (re.++ (re.++ (re.++ (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range (str.++ Domainname "@@") "9")))) (re.+ (re.++ (str.to.re "..") (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9"))))))) (str.to.re "@@")) (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))))))
(assert (not (str.in.re Domainname (re.++ (re.++ (str.to.re "..") (re.* re.allchar)) (re.* re.allchar)))))
(check-sat)
