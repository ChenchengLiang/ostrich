(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.to.int Username) 102))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 36))
(assert (> (str.len Domainname) 0))
(assert (= Email (str.++ (str.++ Domainname "") Username)))
(assert (str.in.re Email (re.union (re.union (re.union (re.* (re.union (re.range "a" "z") (re.++ (re.range "A" "Z") (re.range "0" "9")))) (re.+ (re.union (str.to.re "") (re.* (re.++ (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9"))))))) (str.to.re "`")) (re.* (re.union (re.range "a" "z") (re.++ (re.range "A" "Z") (re.range "0" "9")))))))
(assert (not (str.in.re Domainname (re.++ (re.union (re.+ re.allchar) (str.to.re "k")) (re.+ re.allchar)))))
(check-sat)
