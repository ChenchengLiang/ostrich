(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.len Username) (str.len Username)))
(assert (> 64 0))
(assert (<= (str.len Domainname) 256))
(assert (> 0 (str.len Domainname)))
(assert (= Email "0"))
(assert (str.in.re Email (re.++ (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))) (re.++ (re.* re.allchar) (re.++ (re.+ (re.++ (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))) (str.to.re "."))) (re.+ (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "9" (str.++ Username (str.++ "@" Domainname)))))))))))
(assert (not (str.in.re Domainname (re.++ (re.* re.allchar) (re.++ (str.to.re "@") (str.to.re "."))))))
(check-sat)
