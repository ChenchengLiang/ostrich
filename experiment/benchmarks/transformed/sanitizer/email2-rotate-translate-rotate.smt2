(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.len Username) 64))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 256))
(assert (> (str.len Domainname) 0))
(assert (= Email (str.++ Domainname (str.++ Username "H"))))
(assert (str.in.re Email (re.++ (re.union (re.range "a" "z") (re.+ (re.++ (re.++ (re.union (re.+ (str.to.re "p")) (re.union (re.range "0" "9") (re.+ (re.+ (re.union (re.range "0" "9") (re.union (re.range "a" "z") (re.range "A" "Z"))))))) (str.to.re "H")) (re.++ (re.range "a" "z") (re.range "A" "Z"))))) (re.union (re.range "0" "9") (re.range "A" "Z")))))
(assert (not (str.in.re Domainname (re.++ (str.to.re "p") (re.++ (re.* re.allchar) (re.* re.allchar))))))
(check-sat)
