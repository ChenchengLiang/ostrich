(assert (str.in.re Email (re.++ (re.* (re.union (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))) (re.++ (re.* re.allchar) (re.union (re.+ (re.union (re.* (re.++ (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))) (re.union (str.to.re ";") (re.+ re.allchar)))) (re.+ (re.++ (re.range "a" "z") (re.++ (re.range "A" (str.++ Username (str.++ "@" Domainname))) (re.range "0" "Z")))))))))
