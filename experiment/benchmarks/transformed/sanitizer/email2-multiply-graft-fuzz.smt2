(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= 316 20))
(assert (> (str.to.int Username) 0))
(assert (<= (str.to.int Domainname) (str.len Username)))
(assert (> (str.to.int Domainname) 0))
(assert (= Email (str.++ Username "")))
(assert (str.in.re Email (re.++ (re.+ (re.union (re.range "a" "z") (re.++ (re.range "A" "Z") (re.range "0" "9")))) (re.union (str.to.re "@") (re.union (re.* (re.union (re.* (re.union (re.range "a" "z") (re.++ (re.range "A" "Z") (re.range "0" "9")))) (str.to.re ".-"))) (re.* (re.++ (re.range "a" "z") (re.union (re.range "A" "Z") (re.range (str.++ "@" Domainname) "9")))))))))
(assert (not (str.in.re Domainname (re.++ (re.+ re.allchar) (re.union (re.* re.allchar) (str.to.re "."))))))
(check-sat)
