(declare-const Username String)
(declare-const Domainname String)
(declare-const Email String)
(assert (<= (str.len Username) 180))
(assert (> (str.len Username) 0))
(assert (<= (str.len Domainname) 780))
(assert (> (str.to.int Domainname) 0))
(assert (= Email (str.++ Username (str.++ "" Domainname))))
(assert (str.in.re Email (re.union (re.* (re.++ (re.range "a" "z") (re.++ (re.range "A" "Z") (re.range "0" "9")))) (re.union (str.to.re "") (re.++ (re.* (re.++ (re.* (re.++ (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))) (str.to.re ">7"))) (re.+ (re.++ (re.range "a" "z") (re.union (re.range "A" "Z") (re.range "0" "9")))))))))
(assert (not (str.in.re Domainname (re.union (re.+ re.allchar) (re.union (str.to.re "Q") (re.* re.allchar))))))
(check-sat)
